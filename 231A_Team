#include<iostream>
#include<string>
#include<bits/stdc++.h>
#define max 100
using namespace std;
int main()
{
    int t,a,b,c; 
    int sum=0;
    cin >> t;
    for(int i=0;i<t;i++)
    {
        cin>>a>>b>>c;
        if(a+b+c>=2)
        {
            sum++;
        }
    }
    cout<<sum;
	
	return 0;
}
